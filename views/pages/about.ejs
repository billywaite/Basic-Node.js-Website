<%- contentFor('hero') %>
<section class="hero">
		<div class="hero-content">
		<h2>Welcome to the about page!</h2>
		<p>Learn about our organization here</p>

		<a href="/contact" class="btn btn-lg btn-primary">Contact Us</a>
		</div>
	</section>

<%- contentFor('body') %>
<div class="jumbotron text-center">
	<h1>The Team</h1>

	<div class="row"></div>

	<% for (user of users) { %>
		<div class="col-sm-3">
			<h2><%= user.name %>
			<img src="<%= user.avatar %>" alt="" class="img-responsive">
		</div> 
	<% } %>
	</div>
</div>